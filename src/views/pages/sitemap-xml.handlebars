<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/static/css/sitemap.xsl"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {{#each urls}}
    <url>
        <loc>{{this.loc}}</loc>
        {{#if this.lastmod}}<lastmod>{{this.lastmod}}</lastmod>{{/if}}
        {{#if this.changefreq}}<changefreq>{{this.changefreq}}</changefreq>{{/if}}
        {{#if this.priority}}<priority>{{this.priority}}</priority>{{/if}}
    </url>
    {{/each}}
</urlset>
