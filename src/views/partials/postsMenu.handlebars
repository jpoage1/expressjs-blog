{{!-- partials/postsMenu.handlebars --}}
{{#each years}}
<section class="year-block">
  <h3 class="menu-year">{{year}}</h3>
  <ul class="months-list">
    {{#each months}}
    <li class="menu-month">
      <h4 class="month-label">{{formatMonth month}}</h4>
      <ul class="posts-list">
        {{#each posts}}
        <li class="post-label""><a href=" {{url}}">{{title}}</a></li>
        {{/each}}
      </ul>
    </li>
    {{/each}}
  </ul>
</section>
{{/each}}
